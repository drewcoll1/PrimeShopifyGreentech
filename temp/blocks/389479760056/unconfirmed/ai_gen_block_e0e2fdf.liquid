{% doc %}
  @prompt
    make me a custom footer with search bar and then quck links below it, then next to it have a place for logo and description as well as one social media tag for the website, then in the copyright section have the payment logos as well as in the middle another logo and all 5 social media tags, quick links should just be spots for links

{% enddoc %}
{% assign ai_gen_id = block.id | replace: '_', '' | downcase %}

{% style %}
  .ai-custom-footer-{{ ai_gen_id }} {
    background-color: {{ block.settings.background_color }};
    color: {{ block.settings.text_color }};
    padding: 60px 0 20px;
  }

  .ai-custom-footer-container-{{ ai_gen_id }} {
    max-width: 1200px;
    margin: 0 auto;
    padding: 0 20px;
  }

  .ai-custom-footer-main-{{ ai_gen_id }} {
    display: grid;
    grid-template-columns: 1fr 1fr;
    gap: 60px;
    margin-bottom: 40px;
  }

  .ai-custom-footer-left-{{ ai_gen_id }} {
    display: flex;
    flex-direction: column;
    gap: 30px;
  }

  .ai-custom-footer-search-{{ ai_gen_id }} {
    position: relative;
  }

  .ai-custom-footer-search-input-{{ ai_gen_id }} {
    width: 100%;
    padding: 15px 50px 15px 20px;
    border: 1px solid {{ block.settings.input_border_color }};
    border-radius: {{ block.settings.input_border_radius }}px;
    background-color: {{ block.settings.input_background_color }};
    color: {{ block.settings.input_text_color }};
    font-size: 16px;
  }

  .ai-custom-footer-search-input-{{ ai_gen_id }}::placeholder {
    color: {{ block.settings.input_placeholder_color }};
  }

  .ai-custom-footer-search-button-{{ ai_gen_id }} {
    position: absolute;
    right: 10px;
    top: 50%;
    transform: translateY(-50%);
    background: none;
    border: none;
    color: {{ block.settings.text_color }};
    cursor: pointer;
    padding: 10px;
  }

  .ai-custom-footer-quick-links-{{ ai_gen_id }} {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(150px, 1fr));
    gap: 20px;
  }

  .ai-custom-footer-link-column-{{ ai_gen_id }} {
    display: flex;
    flex-direction: column;
    gap: 10px;
  }

  .ai-custom-footer-link-title-{{ ai_gen_id }} {
    font-weight: 600;
    font-size: 18px;
    margin-bottom: 5px;
    color: {{ block.settings.heading_color }};
  }

  .ai-custom-footer-link-{{ ai_gen_id }} {
    color: {{ block.settings.link_color }};
    text-decoration: none;
    font-size: 14px;
    transition: color 0.3s ease;
  }

  .ai-custom-footer-link-{{ ai_gen_id }}:hover {
    color: {{ block.settings.link_hover_color }};
  }

  .ai-custom-footer-right-{{ ai_gen_id }} {
    display: flex;
    flex-direction: column;
    gap: 20px;
  }

  .ai-custom-footer-logo-{{ ai_gen_id }} {
    max-width: 200px;
    height: auto;
  }

  .ai-custom-footer-logo-placeholder-{{ ai_gen_id }} {
    width: 200px;
    height: 60px;
    background-color: {{ block.settings.text_color }};
    border-radius: 4px;
  }

  .ai-custom-footer-logo-placeholder-{{ ai_gen_id }} svg {
    width: 100%;
    height: 100%;
    opacity: 0.3;
  }

  .ai-custom-footer-description-{{ ai_gen_id }} {
    font-size: 14px;
    line-height: 1.6;
    margin-bottom: 15px;
  }

  .ai-custom-footer-social-single-{{ ai_gen_id }} {
    display: flex;
    align-items: center;
    gap: 10px;
  }

  .ai-custom-footer-social-icon-{{ ai_gen_id }} {
    width: 24px;
    height: 24px;
    color: {{ block.settings.social_icon_color }};
    transition: color 0.3s ease;
  }

  .ai-custom-footer-social-link-{{ ai_gen_id }}:hover .ai-custom-footer-social-icon-{{ ai_gen_id }} {
    color: {{ block.settings.social_icon_hover_color }};
  }

  .ai-custom-footer-social-text-{{ ai_gen_id }} {
    font-size: 14px;
    color: {{ block.settings.text_color }};
  }

  .ai-custom-footer-bottom-{{ ai_gen_id }} {
    border-top: 1px solid {{ block.settings.border_color }};
    padding-top: 20px;
    display: flex;
    align-items: center;
    justify-content: space-between;
    flex-wrap: wrap;
    gap: 20px;
  }

  .ai-custom-footer-payment-{{ ai_gen_id }} {
    display: flex;
    align-items: center;
    gap: 10px;
    flex-wrap: wrap;
  }

  .ai-custom-footer-payment-icon-{{ ai_gen_id }} {
    width: 40px;
    height: 25px;
    background-color: {{ block.settings.payment_bg_color }};
    border: 1px solid {{ block.settings.border_color }};
    border-radius: 4px;
    display: flex;
    align-items: center;
    justify-content: center;
    font-size: 10px;
    color: {{ block.settings.text_color }};
  }

  .ai-custom-footer-center-logo-{{ ai_gen_id }} {
    max-width: 120px;
    height: auto;
  }

  .ai-custom-footer-center-logo-placeholder-{{ ai_gen_id }} {
    width: 120px;
    height: 40px;
    background-color: {{ block.settings.text_color }};
    border-radius: 4px;
  }

  .ai-custom-footer-center-logo-placeholder-{{ ai_gen_id }} svg {
    width: 100%;
    height: 100%;
    opacity: 0.3;
  }

  .ai-custom-footer-social-all-{{ ai_gen_id }} {
    display: flex;
    gap: 15px;
  }

  .ai-custom-footer-social-all-link-{{ ai_gen_id }} {
    color: {{ block.settings.social_icon_color }};
    transition: color 0.3s ease;
  }

  .ai-custom-footer-social-all-link-{{ ai_gen_id }}:hover {
    color: {{ block.settings.social_icon_hover_color }};
  }

  .ai-custom-footer-social-all-icon-{{ ai_gen_id }} {
    width: 24px;
    height: 24px;
  }

  .ai-custom-footer-copyright-{{ ai_gen_id }} {
    font-size: 12px;
    color: {{ block.settings.copyright_color }};
  }

  @media screen and (max-width: 768px) {
    .ai-custom-footer-main-{{ ai_gen_id }} {
      grid-template-columns: 1fr;
      gap: 40px;
    }

    .ai-custom-footer-quick-links-{{ ai_gen_id }} {
      grid-template-columns: repeat(2, 1fr);
    }

    .ai-custom-footer-bottom-{{ ai_gen_id }} {
      flex-direction: column;
      text-align: center;
    }

    .ai-custom-footer-social-all-{{ ai_gen_id }} {
      justify-content: center;
    }
  }

  @media screen and (max-width: 480px) {
    .ai-custom-footer-quick-links-{{ ai_gen_id }} {
      grid-template-columns: 1fr;
    }
  }
{% endstyle %}

<footer class="ai-custom-footer-{{ ai_gen_id }}" {{ block.shopify_attributes }}>
  <div class="ai-custom-footer-container-{{ ai_gen_id }}">
    <div class="ai-custom-footer-main-{{ ai_gen_id }}">
      <div class="ai-custom-footer-left-{{ ai_gen_id }}">
        <div class="ai-custom-footer-search-{{ ai_gen_id }}">
          <form action="{{ routes.search_url }}" method="get" role="search">
            <input
              type="search"
              name="q"
              class="ai-custom-footer-search-input-{{ ai_gen_id }}"
              placeholder="{{ block.settings.search_placeholder }}"
              value="{{ search.terms | escape }}"
              aria-label="Search products"
            >
            <button
              type="submit"
              class="ai-custom-footer-search-button-{{ ai_gen_id }}"
              aria-label="Search"
            >
              <svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round">
                <circle cx="11" cy="11" r="8"></circle>
                <path d="m21 21-4.35-4.35"></path>
              </svg>
            </button>
          </form>
        </div>

        <div class="ai-custom-footer-quick-links-{{ ai_gen_id }}">
          {% if block.settings.link_1_title != blank or block.settings.link_1_url != blank %}
            <div class="ai-custom-footer-link-column-{{ ai_gen_id }}">
              <h3 class="ai-custom-footer-link-title-{{ ai_gen_id }}">{{ block.settings.column_1_title }}</h3>
              {% if block.settings.link_1_title != blank and block.settings.link_1_url != blank %}
                <a href="{{ block.settings.link_1_url }}" class="ai-custom-footer-link-{{ ai_gen_id }}">{{ block.settings.link_1_title }}</a>
              {% endif %}
              {% if block.settings.link_2_title != blank and block.settings.link_2_url != blank %}
                <a href="{{ block.settings.link_2_url }}" class="ai-custom-footer-link-{{ ai_gen_id }}">{{ block.settings.link_2_title }}</a>
              {% endif %}
              {% if block.settings.link_3_title != blank and block.settings.link_3_url != blank %}
                <a href="{{ block.settings.link_3_url }}" class="ai-custom-footer-link-{{ ai_gen_id }}">{{ block.settings.link_3_title }}</a>
              {% endif %}
              {% if block.settings.link_4_title != blank and block.settings.link_4_url != blank %}
                <a href="{{ block.settings.link_4_url }}" class="ai-custom-footer-link-{{ ai_gen_id }}">{{ block.settings.link_4_title }}</a>
              {% endif %}
            </div>
          {% endif %}

          {% if block.settings.link_5_title != blank or block.settings.link_5_url != blank %}
            <div class="ai-custom-footer-link-column-{{ ai_gen_id }}">
              <h3 class="ai-custom-footer-link-title-{{ ai_gen_id }}">{{ block.settings.column_2_title }}</h3>
              {% if block.settings.link_5_title != blank and block.settings.link_5_url != blank %}
                <a href="{{ block.settings.link_5_url }}" class="ai-custom-footer-link-{{ ai_gen_id }}">{{ block.settings.link_5_title }}</a>
              {% endif %}
              {% if block.settings.link_6_title != blank and block.settings.link_6_url != blank %}
                <a href="{{ block.settings.link_6_url }}" class="ai-custom-footer-link-{{ ai_gen_id }}">{{ block.settings.link_6_title }}</a>
              {% endif %}
              {% if block.settings.link_7_title != blank and block.settings.link_7_url != blank %}
                <a href="{{ block.settings.link_7_url }}" class="ai-custom-footer-link-{{ ai_gen_id }}">{{ block.settings.link_7_title }}</a>
              {% endif %}
              {% if block.settings.link_8_title != blank and block.settings.link_8_url != blank %}
                <a href="{{ block.settings.link_8_url }}" class="ai-custom-footer-link-{{ ai_gen_id }}">{{ block.settings.link_8_title }}</a>
              {% endif %}
            </div>
          {% endif %}

          {% if block.settings.link_9_title != blank or block.settings.link_9_url != blank %}
            <div class="ai-custom-footer-link-column-{{ ai_gen_id }}">
              <h3 class="ai-custom-footer-link-title-{{ ai_gen_id }}">{{ block.settings.column_3_title }}</h3>
              {% if block.settings.link_9_title != blank and block.settings.link_9_url != blank %}
                <a href="{{ block.settings.link_9_url }}" class="ai-custom-footer-link-{{ ai_gen_id }}">{{ block.settings.link_9_title }}</a>
              {% endif %}
              {% if block.settings.link_10_title != blank and block.settings.link_10_url != blank %}
                <a href="{{ block.settings.link_10_url }}" class="ai-custom-footer-link-{{ ai_gen_id }}">{{ block.settings.link_10_title }}</a>
              {% endif %}
              {% if block.settings.link_11_title != blank and block.settings.link_11_url != blank %}
                <a href="{{ block.settings.link_11_url }}" class="ai-custom-footer-link-{{ ai_gen_id }}">{{ block.settings.link_11_title }}</a>
              {% endif %}
              {% if block.settings.link_12_title != blank and block.settings.link_12_url != blank %}
                <a href="{{ block.settings.link_12_url }}" class="ai-custom-footer-link-{{ ai_gen_id }}">{{ block.settings.link_12_title }}</a>
              {% endif %}
            </div>
          {% endif %}
        </div>
      </div>

      <div class="ai-custom-footer-right-{{ ai_gen_id }}">
        {% if block.settings.logo %}
          <img
            src="{{ block.settings.logo | image_url: width: 400 }}"
            alt="{{ shop.name }}"
            class="ai-custom-footer-logo-{{ ai_gen_id }}"
            loading="lazy"
            width="{{ block.settings.logo.width }}"
            height="{{ block.settings.logo.height }}"
          >
        {% else %}
          <div class="ai-custom-footer-logo-placeholder-{{ ai_gen_id }}">
            {{ 'image' | placeholder_svg_tag }}
          </div>
        {% endif %}

        {% if block.settings.description != blank %}
          <div class="ai-custom-footer-description-{{ ai_gen_id }}">
            {{ block.settings.description }}
          </div>
        {% endif %}

        {% if block.settings.social_platform != blank and block.settings.social_url != blank %}
          <div class="ai-custom-footer-social-single-{{ ai_gen_id }}">
            <a href="{{ block.settings.social_url }}" class="ai-custom-footer-social-link-{{ ai_gen_id }}" target="_blank" rel="noopener">
              {% case block.settings.social_platform %}
                {% when 'facebook' %}
                  <svg class="ai-custom-footer-social-icon-{{ ai_gen_id }}" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="currentColor">
                    <path d="M24 12.073c0-6.627-5.373-12-12-12s-12 5.373-12 12c0 5.99 4.388 10.954 10.125 11.854v-8.385H7.078v-3.47h3.047V9.43c0-3.007 1.792-4.669 4.533-4.669 1.312 0 2.686.235 2.686.235v2.953H15.83c-1.491 0-1.956.925-1.956 1.874v2.25h3.328l-.532 3.47h-2.796v8.385C19.612 23.027 24 18.062 24 12.073z"/>
