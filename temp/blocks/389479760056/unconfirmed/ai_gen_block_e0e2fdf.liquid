            </a>
            <span class="ai-custom-footer-social-text-{{ ai_gen_id }}">{{ block.settings.social_text }}</span>
          </div>
        {% endif %}
      </div>
    </div>

    <div class="ai-custom-footer-bottom-{{ ai_gen_id }}">
      <div class="ai-custom-footer-payment-{{ ai_gen_id }}">
        {% if block.settings.show_payment_icons %}
          <div class="ai-custom-footer-payment-icon-{{ ai_gen_id }}">VISA</div>
          <div class="ai-custom-footer-payment-icon-{{ ai_gen_id }}">MC</div>
          <div class="ai-custom-footer-payment-icon-{{ ai_gen_id }}">AMEX</div>
          <div class="ai-custom-footer-payment-icon-{{ ai_gen_id }}">DISC</div>
          <div class="ai-custom-footer-payment-icon-{{ ai_gen_id }}">PAYPAL</div>
        {% endif %}
        {% if block.settings.copyright_text != blank %}
          <span class="ai-custom-footer-copyright-{{ ai_gen_id }}">{{ block.settings.copyright_text }}</span>
        {% endif %}
      </div>

      {% if block.settings.center_logo %}
        <img
          src="{{ block.settings.center_logo | image_url: width: 240 }}"
          alt="{{ shop.name }}"
          class="ai-custom-footer-center-logo-{{ ai_gen_id }}"
          loading="lazy"
          width="{{ block.settings.center_logo.width }}"
          height="{{ block.settings.center_logo.height }}"
        >
      {% else %}
        <div class="ai-custom-footer-center-logo-placeholder-{{ ai_gen_id }}">
          {{ 'image' | placeholder_svg_tag }}
        </div>
      {% endif %}

      <div class="ai-custom-footer-social-all-{{ ai_gen_id }}">
        {% if block.settings.facebook_url != blank %}
          <a href="{{ block.settings.facebook_url }}" class="ai-custom-footer-social-all-link-{{ ai_gen_id }}" target="_blank" rel="noopener" aria-label="Facebook">
            <svg class="ai-custom-footer-social-all-icon-{{ ai_gen_id }}" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="currentColor">
              <path d="M24 12.073c0-6.627-5.373-12-12-12s-12 5.373-12 12c0 5.99 4.388 10.954 10.125 11.854v-8.385H7.078v-3.47h3.047V9.43c0-3.007 1.792-4.669 4.533-4.669 1.312 0 2.686.235 2.686.235v2.953H15.83c-1.491 0-1.956.925-1.956 1.874v2.25h3.328l-.532 3.47h-2.796v8.385C19.612 23.027 24 18.062 24 12.073z"/>
            </svg>
          </a>
        {% endif %}

        {% if block.settings.instagram_url != blank %}
          <a href="{{ block.settings.instagram_url }}" class="ai-custom-footer-social-all-link-{{ ai_gen_id }}" target="_blank" rel="noopener" aria-label="Instagram">
            <svg class="ai-custom-footer-social-all-icon-{{ ai_gen_id }}" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="currentColor">
