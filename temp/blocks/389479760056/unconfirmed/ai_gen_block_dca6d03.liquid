{% doc %}
  @prompt
    please make me a custom footer let there be a section for search with bar, also show all the tabs in the nav at the bottom, let there be a logo and an area for text, as well as icons for social media pages, make the footer span the entire width of the page, please add in social media icons

{% enddoc %}
{% assign ai_gen_id = block.id | replace: '_', '' | downcase %}

{% style %}
  .ai-custom-footer-{{ ai_gen_id }} {
    background-color: {{ block.settings.background_color }};
    color: {{ block.settings.text_color }};
    padding: {{ block.settings.padding_top }}px 0 {{ block.settings.padding_bottom }}px;
    width: 100%;}

  .ai-custom-footer__container-{{ ai_gen_id }} {
    max-width: 1200px;
    margin: 0 auto;
    padding: 0 20px;
  }

  .ai-custom-footer__content-{{ ai_gen_id }} {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));
    gap: 40px;
    margin-bottom: 40px;
  }

  .ai-custom-footer__section-{{ ai_gen_id }} {
    display: flex;
    flex-direction: column;
    gap: 16px;
  }

  .ai-custom-footer__logo-{{ ai_gen_id }} {
    max-width: {{ block.settings.logo_width }}px;
    height: auto;
  }

  .ai-custom-footer__logo-placeholder-{{ ai_gen_id }} {
    width: {{ block.settings.logo_width }}px;
    height: 60px;
    background-color: {{ block.settings.text_color }};
    opacity: 0.1;
    border-radius: 4px;
    display: flex;
    align-items: center;
    justify-content: center;
  }

  .ai-custom-footer__logo-placeholder-{{ ai_gen_id }} svg {
    width: 80%;
    height: 80%;
    fill: {{ block.settings.text_color }};
  }

  .ai-custom-footer__text-{{ ai_gen_id }} {
    font-size: {{ block.settings.text_size }}px;
    line-height: 1.5;
    margin: 0;
  }

  .ai-custom-footer__search-form-{{ ai_gen_id }} {
    display: flex;
    gap: 8px;
    margin-top: 8px;
  }

  .ai-custom-footer__search-input-{{ ai_gen_id }} {
    flex: 1;
    padding: 12px 16px;
    border: 1px solid {{ block.settings.input_border_color }};
    border-radius: {{ block.settings.input_border_radius }}px;
    background-color: {{ block.settings.input_background_color }};
    color: {{ block.settings.text_color }};font-size: 14px;
  }

  .ai-custom-footer__search-input-{{ ai_gen_id }}::placeholder {
    color: {{ block.settings.text_color }};
    opacity: 0.6;
  }

  .ai-custom-footer__search-button-{{ ai_gen_id }} {
    padding: 12px 20px;
    background-color: {{ block.settings.button_color }};
    color: {{ block.settings.button_text_color }};
    border: none;
    border-radius: {{ block.settings.button_border_radius }}px;
    cursor: pointer;
    font-size: 14px;
    transition: background-color 0.3s ease;
  }

  .ai-custom-footer__search-button-{{ ai_gen_id }}:hover {
    background-color: {{ block.settings.button_hover_color }};
  }

  .ai-custom-footer__nav-{{ ai_gen_id }} {
    display: flex;
    flex-direction: column;
    gap: 12px;
  }

  .ai-custom-footer__nav-link-{{ ai_gen_id }} {
    color: {{ block.settings.text_color }};
    text-decoration: none;
    font-size: {{ block.settings.nav_text_size }}px;
    transition: color 0.3s ease;
  }

  .ai-custom-footer__nav-link-{{ ai_gen_id }}:hover {
    color: {{ block.settings.link_hover_color }};
  }

  .ai-custom-footer__social-{{ ai_gen_id }} {
    display: flex;
    gap: 16px;
    margin-top: 8px;
    flex-wrap: wrap;
  }

  .ai-custom-footer__social-link-{{ ai_gen_id }} {
    display: flex;
    align-items: center;
    justify-content: center;
    width: {{ block.settings.social_icon_size }}px;
    height: {{ block.settings.social_icon_size }}px;
    background-color: {{ block.settings.social_icon_background }};
    color: {{ block.settings.social_icon_color }};
    border-radius: {{ block.settings.social_icon_border_radius }}px;
    text-decoration: none;
    transition: all 0.3s ease;
  }

  .ai-custom-footer__social-link-{{ ai_gen_id }}:hover {
    background-color: {{ block.settings.social_icon_hover_background }};
    color: {{ block.settings.social_icon_hover_color }};
    transform: translateY(-2px);
  }

  .ai-custom-footer__social-link-{{ ai_gen_id }} svg {
    width: calc({{ block.settings.social_icon_size }}px * 0.6);
    height: calc({{ block.settings.social_icon_size }}px * 0.6);fill: currentColor;
  }

  .ai-custom-footer__bottom-{{ ai_gen_id }} {
    border-top: 1px solid {{ block.settings.border_color }};
    padding-top: 20px;
    text-align: center;
    font-size: {{ block.settings.copyright_text_size }}px;
    opacity: 0.8;
  }

  @media screen and (max-width: 768px) {
    .ai-custom-footer__content-{{ ai_gen_id }} {
      grid-template-columns: 1fr;
      gap: 30px;
    }

    .ai-custom-footer__search-form-{{ ai_gen_id }} {
      flex-direction: column;}

    .ai-custom-footer__social-{{ ai_gen_id }} {
      justify-content: center;
    }
  }
{% endstyle %}

<footer class="ai-custom-footer-{{ ai_gen_id }}" {{ block.shopify_attributes }}>
