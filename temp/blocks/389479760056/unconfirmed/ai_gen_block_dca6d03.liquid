                <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24">
                  <path d="M18.244 2.25h3.308l-7.227 8.26 8.502 11.24H16.17l-5.214-6.817L4.99 21.75H1.68l7.73-8.835L1.254 2.25H8.08l4.713 6.231zm-1.161 17.52h1.833L7.084 4.126H5.117z"/>
                </svg>
              </a>
            {% endif %}
            
            {% if block.settings.youtube_url != blank %}
              <a href="{{ block.settings.youtube_url }}" class="ai-custom-footer__social-link-{{ ai_gen_id }}" target="_blank" rel="noopener" aria-label="YouTube">
                <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24">
                  <path d="M23.498 6.186a3.016 3.016 0 0 0-2.122-2.136C19.505 3.545 12 3.545 12 3.545s-7.505 0-9.377.505A3.017 3.017 0 0 0 .502 6.186C0 8.07 0 12 0 12s0 3.93.502 5.814a3.016 3.016 0 0 0 2.122 2.136c1.871.505 9.376.505 9.376.505s7.505 0 9.377-.505a3.015 3.015 0 0 0 2.122-2.136C24 15.93 24 12 24 12s0-3.93-.502-5.814zM9.545 15.568V8.432L15.818 12l-6.273 3.568z"/>
                </svg>
              </a>
            {% endif %}

            {% if block.settings.tiktok_url != blank %}
              <a href="{{ block.settings.tiktok_url }}" class="ai-custom-footer__social-link-{{ ai_gen_id }}" target="_blank" rel="noopener" aria-label="TikTok">
                <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24">
                  <path d="M12.525.02c1.31-.02 2.61-.01 3.91-.02.08 1.53.63 3.09 1.75 4.171.12 1.11 2.7 1.62 4.24 1.79v4.03c-1.44-.05-2.89-.35-4.2-.97-.57-.26-1.1-.59-1.62-.93-.012.92.01 5.84-.02 8.75-.08 1.4-.54 2.79-1.35 3.94-1.311.92-3.58 3.17-5.91 3.21-1.43.08-2.86-.31-4.08-1.03-2.02-1.19-3.44-3.37-3.65-5.71-.02-.5-.03-1-.01-1.49.18-1.9 1.12-3.72 2.58-4.96 1.66-1.44 3.98-2.13 6.15-1.72.021.48-.04 2.96-.04 4.44-.99-.32-2.15-.23-3.02.37-.63.41-1.111.04-1.36 1.75-.21.51-.15 1.07-.14 1.61.24 1.64 1.82 3.02 3.5 2.87 1.12-.01 2.19-.66 2.77-1.61.19-.33.4-.67.41-1.06.1-1.79.06-3.57.07-5.36.01-4.03-.01-8.05.02-12.07z"/>
                </svg>
              </a>
            {% endif %}

            {% if block.settings.linkedin_url != blank %}
              <a href="{{ block.settings.linkedin_url }}" class="ai-custom-footer__social-link-{{ ai_gen_id }}" target="_blank" rel="noopener" aria-label="LinkedIn">
                <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24">
                  <path d="M20.447 20.452h-3.554v-5.569c0-1.328-.027-3.037-1.852-3.037-1.853 0-2.136 1.445-2.136 2.939v5.667H9.351V9h3.414v1.561h.046c.477-.9 1.637-1.85 3.37-1.85 3.601 0 4.267 2.37 4.267 5.455v6.286zM5.337 7.433c-1.144 0-2.063-.926-2.063-2.065 0-1.138.92-2.063 2.063-2.063 1.14 0 2.064.925 2.064 2.063 0 1.139-.925 2.065-2.064 2.065zm1.782 13.019H3.555V9h3.564v11.452zM22.225 0H1.771C.792 0 0 .774 0 1.729v20.542C0 23.227.792 24 1.771 24h20.451C23.2 24 24 23.227 24 22.271V1.729C24 .774 23.2 0 22.222 0h.003z"/>
                </svg>
              </a>
            {% endif %}

            {% if block.settings.pinterest_url != blank %}
              <a href="{{ block.settings.pinterest_url }}" class="ai-custom-footer__social-link-{{ ai_gen_id }}" target="_blank" rel="noopener" aria-label="Pinterest">
                <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24">
                  <path d="M12.017 0C5.396 0 .029 5.367.029 11.987c0 5.079 3.158 9.417 7.618 11.174-.105-.949-.199-2.403.041-3.439.219-.937 1.406-5.957 1.406-5.957s-.359-.72-.359-1.781c0-1.663.967-2.911 2.168-2.911 1.024 0 1.518.769 1.518 1.688 0 1.029-.653 2.567-.992 3.992-.285 1.193.6 2.165 1.775 2.165 2.128 0 3.768-2.245 3.768-5.487 0-2.861-2.063-4.869-5.008-4.869-3.41 0-5.409 2.562-5.409 5.199 0 1.033.394 2.143.889 2.741.099.12.112.225.085.345-.09.375-.293 1.199-.334 1.363-.053.225-.172.271-.402.165-1.495-.69-2.433-2.878-2.433-4.646 0-3.776 2.748-7.252 7.92-7.252 4.158 0 7.392 2.967 7.392 6.923 0 4.135-2.607 7.462-6.233 7.462-1.214 0-2.357-.629-2.746-1.378l-.748 2.853c-.271 1.043-1.002 2.35-1.492 3.146C9.57 23.812 10.763 24.009 12.017 24c6.624 0 11.99-5.367 11.99-12.013C24.007 5.367 18.641.001 12.017.001z"/>
                </svg>
              </a>
            {% endif %}

            {% if block.settings.snapchat_url != blank %}
              <a href="{{ block.settings.snapchat_url }}" class="ai-custom-footer__social-link-{{ ai_gen_id }}" target="_blank" rel="noopener" aria-label="Snapchat">
                <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24">
                  <path d="M12.017 0C5.396 0 .029 5.367.029 11.987c0 5.079 3.158 9.417 7.618 11.174-.105-.949-.199-2.403.041-3.439.219-.937 1.406-5.957 1.406-5.957s-.359-.72-.359-1.781c0-1.663.967-2.911 2.168-2.911 1.024 0 1.518.769 1.518 1.688 0 1.029-.653 2.567-.992 3.992-.285 1.193.6 2.165 1.775 2.165 2.128 0 3.768-2.245 3.768-5.487 0-2.861-2.063-4.869-5.008-4.869-3.41 0-5.409 2.562-5.409 5.199 0 1.033.394 2.143.889 2.741.099.12.112.225.085.345-.09.375-.293 1.199-.334 1.363-.053.225-.172.271-.402.165-1.495-.69-2.433-2.878-2.433-4.646 0-3.776 2.748-7.252 7.92-7.252 4.158 0 7.392 2.967 7.392 6.923 0 4.135-2.607 7.462-6.233 7.462-1.214 0-2.357-.629-2.746-1.378l-.748 2.853c-.271 1.043-1.002 2.35-1.492 3.146C9.57 23.812 10.763 24.009 12.017 24c6.624 0 11.99-5.367 11.99-12.013C24.007 5.367 18.641.001 12.017.001z"/>
                </svg>
              </a>
            {% endif %}
          </div>
        {% endif %}
      </div>

      {% if block.settings.show_search %}
        <div class="ai-custom-footer__section-{{ ai_gen_id }}">
          <h3>{{ block.settings.search_heading }}</h3>
          <form action="{{ routes.search_url }}" method="get" class="ai-custom-footer__search-form-{{ ai_gen_id }}">
            <input
              type="search"
              name="q"
              class="ai-custom-footer__search-input-{{ ai_gen_id }}"
              placeholder="{{ block.settings.search_placeholder }}"
              value="{{ search.terms | escape }}"
              aria-label="Search"
            >
            <button type="submit" class="ai-custom-footer__search-button-{{ ai_gen_id }}">
              {{ block.settings.search_button_text }}
            </button>
          </form>
        </div>
      {% endif %}

      {% if block.settings.show_navigation %}
        <div class="ai-custom-footer__section-{{ ai_gen_id }}">
          <h3>{{ block.settings.nav_heading }}</h3>
          <nav class="ai-custom-footer__nav-{{ ai_gen_id }}">
            {% for link in linklists[block.settings.menu].links %}
              <a href="{{ link.url }}" class="ai-custom-footer__nav-link-{{ ai_gen_id }}">
                {{ link.title }}
              </a>
            {% endfor %}
          </nav>
        </div>
      {% endif %}
    </div>

    {% if block.settings.show_copyright %}
      <div class="ai-custom-footer__bottom-{{ ai_gen_id }}">
        {{ block.settings.copyright_text }}
      </div>
    {% endif %}
  </div>
</footer>

{% schema %}
{
  "name": "Custom Footer",
  "settings": [
    {
      "type": "header",
      "content": "Logo & Text"
    },
    {
      "type": "image_picker",
      "id": "logo",
      "label": "Logo"
    },
    {
      "type": "range",
      "id": "logo_width",
      "min": 80,
      "max": 300,
      "step": 10,
      "unit": "px",
      "label": "Logo width",
      "default": 150
    },
    {
      "type": "richtext",
      "id": "footer_text",
      "label": "Footer text",
      "default": "<p>Your store description goes here. Tell customers about your brand and what makes you special.</p>"
    },
    {
      "type": "range",
      "id": "text_size",
      "min": 12,
      "max": 20,
      "step": 1,
      "unit": "px",
      "label": "Text size",
      "default": 14
    },
    {
      "type": "header",
      "content": "Search Section"
    },
    {
      "type": "checkbox",
      "id": "show_search",
      "label": "Show search section",
      "default": true
    },
    {
      "type": "text",
      "id": "search_heading",
      "label": "Search heading",
      "default": "Search"
    },
    {
      "type": "text",
      "id": "search_placeholder",
      "label": "Search placeholder text",
      "default": "Search products..."
    },
    {
      "type": "text",
      "id": "search_button_text",
      "label": "Search button text",
      "default": "Search"
    },
    {
      "type": "header",
      "content": "Navigation"
    },
    {
      "type": "checkbox",
      "id": "show_navigation",
      "label": "Show navigation links",
      "default": true
    },
    {
      "type": "text",
      "id": "nav_heading",
      "label": "Navigation heading",
      "default": "Quick Links"
    },
    {
      "type": "link_list",
      "id": "menu",
      "label": "Menu",
      "default": "main-menu"
    },
    {
      "type": "range",
      "id": "nav_text_size",
      "min": 12,
      "max": 18,
      "step": 1,
      "unit": "px",
      "label": "Navigation text size",
      "default": 14
    },
    {
      "type": "header",
      "content": "Social Media"
    },
    {
      "type": "checkbox",
      "id": "show_social",
      "label": "Show social media icons",
      "default": true
    },
    {
      "type": "url",
      "id": "facebook_url",
      "label": "Facebook URL"
    },
    {
      "type": "url",
      "id": "instagram_url",
      "label": "Instagram URL"
    },
    {
      "type": "url",
      "id": "twitter_url",
      "label": "Twitter URL"
    },
    {
      "type": "url",
      "id": "youtube_url",
      "label": "YouTube URL"
    },
    {
      "type": "url",
      "id": "tiktok_url",
      "label": "TikTok URL"
    },
    {
      "type": "url",
      "id": "linkedin_url",
      "label": "LinkedIn URL"
    },
    {
      "type": "url",
      "id": "pinterest_url",
      "label": "Pinterest URL"
    },
    {
      "type": "url",
      "id": "snapchat_url",
      "label": "Snapchat URL"
    },
    {
      "type": "range",
      "id": "social_icon_size",
      "min": 30,
      "max": 60,
      "step": 2,
      "unit": "px",
      "label": "Social icon size",
      "default": 40
    },
    {
      "type": "range",
      "id": "social_icon_border_radius",
      "min": 0,
      "max": 30,
      "step": 2,
      "unit": "px",
      "label": "Social icon border radius",
      "default": 20
    },
    {
      "type": "header",
      "content": "Copyright"
    },
    {
      "type": "checkbox",
      "id": "show_copyright",
      "label": "Show copyright section",
      "default": true
    },
    {
      "type": "richtext",
      "id": "copyright_text",
      "label": "Copyright text",
      "default": "<p>&copy; 2024 Your Store Name. All rights reserved.</p>"
    },
    {
      "type": "range",
      "id": "copyright_text_size",
      "min": 10,
      "max": 16,
      "step": 1,
      "unit": "px",
      "label": "Copyright text size",
      "default": 12
    },
    {
      "type": "header",
      "content": "Colors"
    },
    {
      "type": "color",
      "id": "background_color",
      "label": "Background color",
      "default": "#2e2e2e"
    },
    {
      "type": "color",
      "id": "text_color",
      "label": "Text color",
      "default": "#ffffff"
    },
    {
      "type": "color",
      "id": "link_hover_color",
      "label": "Link hover color",
      "default": "#cccccc"
    },
    {
      "type": "color",
      "id": "border_color",
      "label": "Border color",
      "default": "#555555"
    },
    {
      "type": "color",
      "id": "button_color",
      "label": "Button color",
      "default": "#000f9f"
