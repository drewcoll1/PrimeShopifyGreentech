    <div class="ai-custom-footer__content-{{ ai_gen_id }}">
      <div class="ai-custom-footer__section-{{ ai_gen_id }}">
        {% if block.settings.logo %}
          <img
            src="{{ block.settings.logo | image_url: width: 400}}" 
            alt="{{ shop.name }}"
            class="ai-custom-footer__logo-{{ ai_gen_id }}"width="{{ block.settings.logo_width }}"
            height="auto"
            loading="lazy"
          >
        {% else %}
          <div class="ai-custom-footer__logo-placeholder-{{ ai_gen_id }}">
            {{'image' | placeholder_svg_tag }}
          </div>
        {% endif %}
        
        {% if block.settings.footer_text != blank %}
          <div class="ai-custom-footer__text-{{ ai_gen_id }}">
            {{ block.settings.footer_text }}
          </div>
        {% endif %}

        {% if block.settings.show_social %}
